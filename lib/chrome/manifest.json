{
    "name": "Flint Web Discovery",
    "description": "Flint Web Discovery",
    "version": "0.1",
    "manifest_version": 2,
    "app": {
        "background": {
            "scripts": ["js/discovery/FlintDeviceManager.js", "js/background/background.js"]
        }
    },
    "icons": {
        "16": "img/icon-16.png",
        "128": "img/icon-128.png"
    },
    "sockets": {
        "udp": {
            "send": ":1900",
            "bind": ":1900",
            "multicastMembership": "" // it chrome's bug, cannot be * here
        }
    }
}
